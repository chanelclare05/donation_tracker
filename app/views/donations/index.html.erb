<h1>Donations#index</h1>
<p>Find me in app/views/donations/index.html.erb</p>


<% if @donations.length == 0%>
  <%= "No current Donations" %>
  <% else %>
  <% @donations.each do |d| %>
    Amount <%= d.amount %>
    Date <%= d.amount %>
    Organization <%= d.organization.name %>
  <% end %>
<% end %>